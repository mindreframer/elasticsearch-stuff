# Ignore bundler config.
/.bundle
.rvmrc

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp
*.swp

# Ignore coverage stuff
/coverage
/.coveralls.yml

# Ignore gem builds
*.gem